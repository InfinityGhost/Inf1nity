<UserControl x:Class="Inf1nity_Manager.Controls.ChannelPicker" Name="Control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Inf1nity_Manager.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="18" d:DesignWidth="400"
             DataContext="{Binding ElementName=Control}">
    <ComboBox Name="ChannelsBox" ItemsSource="{Binding Path=Channels}"/>
</UserControl>
